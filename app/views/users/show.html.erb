<h3>Friends</h3>
<table class="table">
  <thead>
    <tr>
      <th scope="col">Name</td>
      <th scope="col">Website</td>
      <th scope="col">Heading</td>
    </tr>
  </thead>
  <tbody>
    <% @friends.each do |f| %>
      <tr>
        <td><%= f.name %></td>
        <td><%= f.website %></td>
        <td><%= link_to "geerup.com/#{f.slug}", "/#{f.slug}", :target => "_blank" %></td>
        <td><%= f.head_one[0] %></td>
      </tr>
    <% end %>
  </tbody>
</table>

